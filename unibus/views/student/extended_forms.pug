doctype html

html(lang='en')
  head
    include ../includes/head
  body
    .wrapper
      include ../includes/sidebar_student
      .main-panel
        include ../includes/navbar
        .content
          .container-fluid
            .card
              .card-content
                .row
                  .col-md-4
                    h4.card-title Datetime Picker
                    .form-group
                      input.form-control.datetimepicker(type='text', placeholder='Datetime Picker Here')
                  .col-md-4
                    h4.card-title Date Picker
                    .form-group
                      input.form-control.datepicker(type='text', placeholder='Date Picker Here')
                  .col-md-4
                    h4.card-title Time Picker
                    .form-group
                      input.form-control.timepicker(type='text', placeholder='Time Picker Here')
                br
                br
                .row
                  .col-md-6
                    h4.card-title Switches
                    .col-md-4
                      p.category
                        | Default
                      input.switch(type='checkbox', checked='')
                      input.switch(type='checkbox')
                    .col-md-4
                      p.category
                        | Plain
                      input.switch-plain(type='checkbox', checked='')
                      input.switch-plain(type='checkbox')
                    .col-md-4
                      p.category
                        | With Icons
                      input.switch-icon(type='checkbox', checked='')
                      input.switch-icon(type='checkbox')
                  .col-md-3
                    h4.card-title Checkboxes
                    .checkbox
                      input#checkbox1(type='checkbox')
                      label(for='checkbox1')
                        | Unchecked
                    .checkbox
                      input#checkbox2(type='checkbox', checked='')
                      label(for='checkbox2')
                        | Checked
                    .checkbox
                      input#checkbox3(type='checkbox', disabled='')
                      label(for='checkbox3')
                        | Disabled unchecked
                    .checkbox
                      input#checkbox4(type='checkbox', checked='', disabled='')
                      label(for='checkbox4')
                        | Disabled checked
                  .col-sm-3
                    h4.card-title Radio
                    .radio
                      input#radio1(type='radio', name='radio1', value='option1')
                      label(for='radio1')
                        | Radio is off
                    .radio
                      input#radio2(type='radio', name='radio1', value='option2', checked='')
                      label(for='radio2')
                        | Radio is on
                    .radio
                      input#radio3(type='radio', name='radio3', value='option1', disabled='')
                      label(for='radio3')
                        | Disabled radio is off
                    .radio
                      input#radio4(type='radio', name='radio4', value='option1', checked='', disabled='')
                      label(for='radio4')
                        | Disabled radio is on
                br
                br
                .row
                  .col-md-6
                    h4.card-title Tags
                    input.tagsinput(type='text', value='Minimal, Light, New, Friends', data-role='tagsinput', data-color='success')
                    // You can change data-color="success" with one of our colors primary | warning | info | danger
                    br
                    h4.card-title Progress Bars
                    .progress
                      .progress-bar(role='progressbar', aria-valuenow='60', aria-valuemin='0', aria-valuemax='100', style='width: 30%;')
                        span.sr-only 60% Complete
                    .progress
                      .progress-bar.progress-bar-info(role='progressbar', aria-valuenow='60', aria-valuemin='0', aria-valuemax='100', style='width: 60%;')
                        span.sr-only 60% Complete
                    .progress
                      .progress-bar.progress-bar-success(style='width: 35%')
                        span.sr-only 35% Complete (success)
                      .progress-bar.progress-bar-warning(style='width: 20%')
                        span.sr-only 20% Complete (warning)
                      .progress-bar.progress-bar-danger(style='width: 10%')
                        span.sr-only 10% Complete (danger)
                    br
                    h4.card-title Sliders
                    #sliderRegular.slider.slider-success
                    br
                    #sliderDouble.slider.slider-info
                  .col-md-6
                    h4.card-title Customisable Select
                    .row
                      .col-sm-6
                        select.selectpicker(data-style='btn btn-danger btn-block', title='Single Select', data-size='7')
                          option(value='id') Bahasa Indonesia
                          option(value='ms') Bahasa Melayu
                          option(value='ca') Català
                          option(value='da') Dansk
                          option(value='de') Deutsch
                          option(value='en') English
                          option(value='es') Español
                          option(value='el') Eλληνικά
                          option(value='fr') Français
                          option(value='it') Italiano
                          option(value='hu') Magyar
                          option(value='nl') Nederlands
                          option(value='no') Norsk
                          option(value='pl') Polski
                          option(value='pt') Português
                          option(value='fi') Suomi
                          option(value='sv') Svenska
                          option(value='tr') Türkçe
                          option(value='is') Íslenska
                          option(value='cs') Čeština
                          option(value='ru') Русский
                          option(value='th') ภาษาไทย
                          option(value='zh') 中文 (简体)
                          option(value='zh-TW') 中文 (繁體)
                          option(value='ja') 日本語
                          option(value='ko') 한국어
                      .col-sm-6
                        select.selectpicker(multiple='', title='Multiple Select', data-style='btn-info btn-fill btn-block', data-size='7')
                          option(value='ARS') ARS
                          option(value='AUD') AUD
                          option(value='BRL') BRL
                          option(value='CAD') CAD
                          option(value='CHF') CHF
                          option(value='CNY') CNY
                          option(value='CZK') CZK
                          option(value='DKK') DKK
                          option(value='EUR') EUR
                          option(value='GBP') GBP
                          option(value='HKD') HKD
                          option(value='HUF') HUF
                          option(value='IDR') IDR
                          option(value='ILS') ILS
                          option(value='INR') INR
                          option(value='JPY') JPY
                          option(value='KRW') KRW
                          option(value='MYR') MYR
                          option(value='MXN') MXN
                          option(value='NOK') NOK
                          option(value='NZD') NZD
                          option(value='PHP') PHP
                          option(value='PLN') PLN
                          option(value='RUB') RUB
                          option(value='SEK') SEK
                          option(value='SGD') SGD
                          option(value='TWD') TWD
                          option(value='USD') USD
                          option(value='VND') VND
                          option(value='ZAR') ZAR
                      .col-sm-6
                        br
                        h4.card-title Dropdown
                        .dropdown
                          button.btn.btn-block.dropdown-toggle(href='#', data-toggle='dropdown')
                            | Regular 
                            b.caret
                          ul.dropdown-menu
                            li
                              a(href='#') Action
                            li
                              a(href='#') Another action
                            li
                              a(href='#') Something else here
                            li.divider
                            li
                              a(href='#') Separated link
                            li.divider
                            li
                              a(href='#') One more separated link
                      .col-sm-6
                        br
                        h4.card-title Dropup
                        .dropup
                          button.btn.btn-block.dropdown-toggle(href='#', data-toggle='dropdown')
                            | Regular 
                            b.caret
                          ul.dropdown-menu
                            li
                              a(href='#') Action
                            li
                              a(href='#') Another action
                            li
                              a(href='#') Something else here
                            li.divider
                            li
                              a(href='#') Separated link
                            li.divider
                            li
                              a(href='#') One more separated link
                br
                br
            // end card
        include ../includes/footer
    include ../includes/scripts
    script(type='text/javascript').
      $().ready(function() {
      // Init Sliders
      demo.initFormExtendedSliders();
      // Init DatetimePicker
      demo.initFormExtendedDatetimepickers();
      });
